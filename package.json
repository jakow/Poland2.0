  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start build/index.js --name app",
    "start:staging": "cross-env NODE_ENV=staging pm2 start build/index.js --name app",
    "stop": "pm2 stop app && pm2 delete app",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "nodemon --watch \"**/*.ts\" --ignore \"client/**/*\" --exec \"ts-node --project @config/tsconfig.server.json\" index.ts",
    "dev:client": "webpack --config @config/client.config.js -w",
    "build:server": "cross-env NODE_ENV=production tsc --project @config/tsconfig.server.json",
    "build:client": "cross-env NODE_ENV=production webpack --config @config/client.config.js -p",
    "build": "yarn run clean && yarn build:server && yarn build:client",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "clean": "rimraf build/*",
    "deploy:staging": "yarn build && tools/deploy-staging.sh",
    "deploy:production": "yarn build && tools/deploy-production.sh"
  },
  "main": "keystone.js"
}
