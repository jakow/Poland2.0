  },
  "engines": {
    "node": ">=7.9.0",
    "yarn": ">=0.23.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production forever build/index.js",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "nodemon --watch \"**/*.ts\" --ignore \"client/**/*\" --exec \"ts-node\" index.ts",
    "dev:client": "webpack-dev-server --config @config/client.config.js -w",
    "build:server": "cross-env NODE_ENV=production webpack --config @config/server.config.js -p --progress",
    "build:client": "cross-env NODE_ENV=production webpack --config @config/client.config.js -p --progress",
    "build": "concurrently \"yarn build:server\" \"yarn build:client\"",
    "test": "mocha $(find test -name '*.js')",
    "watch:server": "webpack --config @config/server.config.js -w",
    "clean": "rimraf build/"
  },
  "main": "keystone.js"
}
