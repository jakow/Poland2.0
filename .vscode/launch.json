{
"version": "0.2.0",
"configurations": [
  {
    "type": "node",
    "request": "launch",
    "name": "Backend launch",
    "program": "${workspaceRoot}/build/index.js",
    "sourceMaps": true,
    "preLaunchTask": "build server",
    "protocol": "inspector"
  },
  {
    "type": "node",
    "request": "attach",
    "name": "Backend attach",
    "processId": "${command:PickProcess}",
    "port": 5859
  },
  {
    "type": "chrome",
    "request": "launch",
    "name": "Frontend launch",
    "url": "http://localhost:8000",
    "webRoot": "${workspaceRoot}",
    "sourceMaps": true
  },
  {
    "type": "chrome",
    "request": "attach",
    "name": "Frontend attach",
    "port": 9222,
    "sourceMaps": true,
    "webRoot": "${workspaceRoot}"
    }
  ]
}